{
  "name": "ussd-assistant",
  "version": "0.1.0",
  "description": "A simple USSD app",
  "scripts": {
    "start": "npm run transpile:server && node ./dist/app.js",
    "start:devserver": "NODE_ENV=development nodemon ./server/app.js --exec babel-node",
    "transpile:server": "rimraf dist && babel server --out-dir dist",
    "build": "webpack",
    "lint": "./node_modules/eslint/bin/eslint.js ./ -- --watch"
  },
  "dependencies": {
    "babel-loader": "^6.4.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-cli": "^6.23.0",
    "body-parser": "^1.17.1",
    "bootstrap": "^3.3.7",
    "eventsource-polyfill": "0.9.6",
    "express": "^4.15.2",
    "fm-log": "^4.4.3",
    "jquery": "^3.1.1",
    "morgan": "^1.8.1",
    "react": "15.0.2",
    "react-dom": "15.0.2",
    "react-router": "2.4.0",
    "rimraf": "^2.6.1",
    "webpack": "2.1.0-beta.21",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.10.0"
  },
  "devDependencies": {
    "css-loader": "^0.27.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "file-loader": "^0.10.1",
    "nodemon": "^1.11.0",
    "style-loader": "^0.13.2",
    "url-loader": "^0.5.8"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andela-aolaniran/cp3-document-management-system.git"
  },
  "keywords": [
    "USSD",
    "Mobile",
    "Phones",
    "api"
  ],
  "author": "Azeez Olaniran",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andela-aolaniran/cp3-document-management-system/issues"
