{
    "extends": "airbnb",
    "plugins": [
      "react",
      "jsx-a11y",
      "import"
    ],
    "env": {
      "browser": true,
      "node": true,
      "jquery": true,
      "mocha": true
    },
    "rules": {
      "react/forbid-prop-types": 0,
      "import/no-unresolved": 0,
      "import/extensions": 0,
      "comma-dangle": 0
    }
}